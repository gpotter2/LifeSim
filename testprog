struct Oiseau {
    x: 0.0;
    y: 0.0;
    vx: 0.0;
    vy: 0.0;
    ax: 0.0;
    ay: 0.0;
}

let close a b = (a.x - b.x) < 10.0 in
Oiseau x <=> Oiseau list L if close x do {
    // Actions Ã  effectuer, modifier le comportement.
    // L contient la liste des plus proches oiseaux
    let ax = 0.0 in
    let ay = 0.0 in
    Oiseau {
        x: x.x + x.vx;
        y: x.y + x.vy;
        vx: x.vx + x.ax;
        vy: x.vy + x.ay;
        ax: ax;
        ay: ay;
    }
}

init {
    Oiseau: 10;
};